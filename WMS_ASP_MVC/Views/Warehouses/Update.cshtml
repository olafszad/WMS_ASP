@model WMS_ASP_MVC.Models.Warehouse


<br />

<h2 class="text-info">Edit Warehouse</h2>
<br />

<div class="border container" style="padding:30px">
    <form method="post">
        <input type="hidden" asp-for="id" />
        <div class="text-danger" asp-validation-summary="ModelOnly"></div>
        <div class="form-group row">
            <div class="col-3">
                <label asp-for="warehouseName"></label>
            </div>
            <div class="col-6">
                <input asp-for="warehouseName" class="form-control" />
            </div>
            <span asp-validation-for="warehouseName" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-3">
                <label asp-for="companyID"></label>
            </div>
            <div class="col-6">
                <input asp-for="companyID" class="form-control" />
            </div>
            <span asp-validation-for="companyID" class="text-danger"></span>
        </div>

        <div class="form-group row">
            <div class="col-3 offset-3">
                <input type="submit" value="Update" class="btn btn-primary form-control" />
            </div>
            <div class="col-3">
                <a asp-action="Index " class="btn btn-success form-control">Back To list</a>

            </div>
        </div>
    </form>
</div>

@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}

